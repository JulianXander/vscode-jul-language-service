{
	"comments": {
		"lineComment": "#"
	},
	"brackets": [
		[
			"(",
			")"
		]
	],
	"autoClosingPairs": [
		{
			"open": "(",
			"close": ")"
		},
		{
			"open": "ยง",
			"close": "ยง"
		}
	],
	"autoCloseBefore": ";:.,=}])>` \n\t",
	"surroundingPairs": [
		[
			"(",
			")"
		],
		[
			"ยง",
			"ยง"
		]
	],
	"folding": {
		"markers": {
			"start": "s*#region\\b",
			"end": "s*#endregion\\b"
		}
	},
	"wordPattern": "(-?\\d*\\.\\d\\w*)|([^\\`\\~\\!\\@\\#\\%\\^\\&\\*\\(\\)\\-\\=\\+\\[\\{\\]\\}\\\\\\|\\;\\:\\'\\\"\\,\\.\\<\\>\\/\\?\\s]+)",
	"indentationRules": {
		"increaseIndentPattern": "(=>|\\?|\\()$",
		"decreaseIndentPattern": "^\\s\\)"
	}
}